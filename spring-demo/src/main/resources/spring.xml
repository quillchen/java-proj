<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.quill.springdemo.bean.Person" id="person">
        <property name="name" value="Jack"></property>
        <property name="age" value="18"></property>
    </bean>

    <bean class="com.quill.springdemo.bean.Computer" id="comp">
        <property name="cpu" value="4"></property>
        <property name="memory" value="16"></property>
        <property name="hardDisk" value="512"></property>
    </bean>

</beans>